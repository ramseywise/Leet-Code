    "def twoSum(numbers, target):\n",
    "    left, right = 0, len(numbers) - 1\n",
    "    sum_num = numbers[left] + numbers[right]\n",
    "    while sum_num != target:\n",
    "        if numbers[right] > target:\n",
    "            right -= 1\n",
    "        else:\n",
    "            left += 1\n",
    "        sum_num = numbers[left] + numbers[right]\n",
    "    return (left+1, right+1)"
